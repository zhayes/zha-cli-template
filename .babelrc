{
    "env":{
        "development": {
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react"
            ],
            "plugins": [
                "react-hot-loader/babel",
                "@babel/plugin-syntax-dynamic-import",
                [
                    "import", {
                        "libraryName": "antd",
                        "libraryDirectory": "es",
                        "style": true
                    }
                ]
            ]
        },
        "production":{
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react"
            ],
            "plugins": [
                "@babel/plugin-syntax-dynamic-import",
                [
                    "import", {
                        "libraryName": "antd",
                        "libraryDirectory": "es",
                        "style": true
                    }
                ]
            ]
        }
    }
}